
<div class="container add_frame">
	<h1>Frames</h1>

	<div class="menu">
		<a href="add">new frame</a>
		[% IF user_roles.import %]
		<a href="import/kris/">import</a>
		[% END %]
	</div>

	<table>

		[% FOREACH frame IN frames %]
		<tr>
			<td><a href="[% frame.id %]">[% frame.name | html %]</a></td>
		</tr>
		[% END %]
	</table>

</div>
