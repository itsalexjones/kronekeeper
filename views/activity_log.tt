
<div class="container frame">
	<h1>
		<a href="[% request.uri_base %]/frame/[% frame_info.id %]">[% frame_info.name | html %]</a>
	</h1>

	<table>
		<tr>
			<th>timestamp</th>
			<th>by user</th>
			<th>activity</th>
		</tr>
		[% FOREACH entry IN activity_log %]
		<tr>
			<td>[% entry.log_timestamp | html %]</td>
			<td>[% entry.by_person_name | html %]</td>
			<td>[% entry.note | html %]</td>
		</tr>
		[% END %]
	</table>
</div>

